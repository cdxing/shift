<?xml version="1.0" encoding="utf-8" ?>

<job maxFilesPerProcess="1" fileListSyntax="xrootd" >

  <command>
    starver SL21c
    ln -s /star/u/dchen/ana/19gev_2019/shift/StRoot .
    ln -s /star/u/dchen/ana/19gev_2019/shift/analyzePico.C .
    ln -s /star/u/dchen/ana/19gev_2019/shift/config_19p6GeV.txt .

    cons
    root4star -q -l -b analyzePico.C\(\"$FILELIST\",\"$JOBID\",\"config_19p6GeV.txt\"\) &gt;&amp; ${JOBID}.log
    tar -zcvf ${JOBID}.log.tgz ${JOBID}.log
  </command>

  <SandBox installer="ZIP">
    <Package>
      <File>file:./analyzePico.C</File>
      <File>file:./config_19p6GeV.txt</File>
      <File>file:./StRoot</File>
    </Package>
  </SandBox>

   <!--input URL="filelist:/star/u/dchen/4p5GeVPico.list" nFiles="all"/-->
   <!--input URL="catalog:star.bnl.gov?production=P19ie,filetype=daq_reco_picoDst,trgsetupname=production_3p85GeV_fixedTarget_2018,tpx=1,filename~st_physics,runnumber=&run;,sanity=1,storage!=HPSS" nFiles="all"/-->
   <input URL="catalog:star.bnl.gov?production=P21ic,filetype=daq_reco_picoDst,trgsetupname=production_19GeV_2019,tpx=1,filename~st_physics,sanity=1,storage!=HPSS" nFiles="103690"/>
   
  <stdout discard="true" />
  <stderr URL="file:./err/$JOBID.err"/>

  <Generator>
	  <Location>./report/</Location>
	  <ScriptLocation>./csh/</ScriptLocation>
	  <ListLocation>./list/</ListLocation>
  </Generator>

  <output fromScratch="*.root" toURL="file:/star/data01/pwg/dchen/Ana/19p6GeV/shift/production/" />
  <output fromScratch="*.log.tgz" toURL="file:/star/data01/pwg/dchen/Ana/19p6GeV/shift/log/" />


</job>

